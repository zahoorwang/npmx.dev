{
  "name": "npmx",
  "license": "MIT",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "author": {
    "name": "Daniel Roe",
    "email": "daniel@roe.dev",
    "url": "https://roe.dev"
  },
  "scripts": {
    "build": "nuxt build",
    "build:lunaria": "node ./lunaria/lunaria.ts",
    "dev": "nuxt dev",
    "dev:docs": "pnpm run --filter npmx-docs dev --port=3001",
    "i18n:check": "node scripts/compare-translations.ts",
    "i18n:check:fix": "node scripts/compare-translations.ts --fix",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "lint": "oxlint && oxfmt --check",
    "lint:fix": "oxlint --fix && oxfmt",
    "generate": "nuxt generate",
    "npmx-connector": "pnpm --filter npmx-connector dev",
    "generate-pwa-icons": "pwa-assets-generator",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && simple-git-hooks && pnpm generate:lexicons",
    "generate:lexicons": "lex build --lexicons lexicons --out shared/types/lexicons --clear",
    "test": "vite test",
    "test:browser": "playwright test",
    "test:browser:ui": "playwright test --ui",
    "test:browser:update": "playwright test --update-snapshots",
    "test:nuxt": "vite test --project nuxt",
    "test:types": "nuxt prepare && vue-tsc -b --noEmit && pnpm --filter npmx-connector test:types",
    "test:unit": "vite test --project unit",
    "start:playwright:webserver": "NODE_ENV=test pnpm build && pnpm preview --port 5678"
  },
  "dependencies": {
    "@atproto/api": "^0.18.17",
    "@atproto/lex": "0.0.13",
    "@atproto/oauth-client-node": "^0.3.15",
    "@deno/doc": "jsr:^0.189.1",
    "@floating-ui/vue": "1.1.10",
    "@iconify-json/carbon": "1.2.18",
    "@iconify-json/lucide": "1.2.87",
    "@iconify-json/simple-icons": "1.2.68",
    "@iconify-json/solar": "1.2.5",
    "@iconify-json/svg-spinners": "1.2.4",
    "@iconify-json/vscode-icons": "1.2.40",
    "@intlify/core-base": "11.2.8",
    "@intlify/shared": "11.2.8",
    "@lunariajs/core": "https://pkg.pr.new/lunariajs/lunaria/@lunariajs/core@f07e1a3",
    "@nuxt/a11y": "1.0.0-alpha.1",
    "@nuxt/fonts": "0.13.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "https://pkg.pr.new/@nuxt/test-utils@1499a48",
    "@nuxtjs/color-mode": "4.0.0",
    "@nuxtjs/html-validator": "2.1.0",
    "@nuxtjs/i18n": "10.2.1",
    "@shikijs/langs": "3.21.0",
    "@shikijs/themes": "3.21.0",
    "@unocss/nuxt": "66.6.0",
    "@unocss/preset-wind4": "66.6.0",
    "@upstash/redis": "1.36.1",
    "@vercel/kv": "3.0.0",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vite-pwa/nuxt": "1.1.0",
    "@voidzero-dev/vite-plus-core": "0.0.0-833c515fa25cef20905a7f9affb156dfa6f151ab",
    "@vueuse/core": "14.2.0",
    "@vueuse/integrations": "14.2.0",
    "@vueuse/nuxt": "14.2.0",
    "@vueuse/router": "^14.2.0",
    "focus-trap": "^7.8.0",
    "marked": "17.0.1",
    "module-replacements": "2.11.0",
    "nuxt": "4.3.0",
    "nuxt-og-image": "5.1.13",
    "ofetch": "1.5.1",
    "perfect-debounce": "2.1.0",
    "sanitize-html": "2.17.0",
    "semver": "7.7.3",
    "shiki": "3.21.0",
    "simple-git": "3.30.0",
    "spdx-license-list": "6.11.0",
    "std-env": "3.10.0",
    "ufo": "1.6.3",
    "unocss": "66.6.0",
    "unplugin-vue-router": "0.19.2",
    "valibot": "1.2.0",
    "validate-npm-package-name": "7.0.2",
    "virtua": "0.48.5",
    "vite-plugin-pwa": "1.2.0",
    "vite-plus": "0.0.0-833c515fa25cef20905a7f9affb156dfa6f151ab",
    "vue": "3.5.27",
    "vue-data-ui": "3.14.2"
  },
  "devDependencies": {
    "@npm/types": "2.1.0",
    "@playwright/test": "1.58.1",
    "@types/node": "24.10.9",
    "@types/sanitize-html": "2.16.0",
    "@types/semver": "7.7.1",
    "@types/validate-npm-package-name": "4.0.2",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "axe-core": "4.11.1",
    "fast-npm-meta": "1.0.0",
    "knip": "5.83.0",
    "lint-staged": "16.2.7",
    "oxfmt": "0.27.0",
    "oxlint": "1.42.0",
    "playwright-core": "1.58.0",
    "schema-dts": "1.1.5",
    "simple-git-hooks": "2.13.1",
    "typescript": "5.9.3",
    "vitest": "npm:@voidzero-dev/vite-plus-test@0.0.0-833c515fa25cef20905a7f9affb156dfa6f151ab",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "3.2.4"
  },
  "engines": {
    "node": ">=24"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "i18n/locales/*": [
      "pnpm build:lunaria",
      "pnpm oxfmt lunaria/files/",
      "git add lunaria/files/"
    ],
    "*.{js,ts,mjs,cjs,vue}": [
      "pnpm oxlint --fix"
    ],
    "*.{js,ts,mjs,cjs,vue,json,yml,md,html,css}": [
      "pnpm oxfmt"
    ]
  },
  "packageManager": "pnpm@10.28.2"
}
